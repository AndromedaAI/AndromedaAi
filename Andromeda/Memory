# memory.py – Andromeda's trade memory
from PIL import Image
import base64
from io import BytesIO

from Trade_validator import validate_trade,validate_alt_trade




def image_to_base64(path):
    img = Image.open(path)
    buffered = BytesIO()
    img.save(buffered, format="PNG")
    return base64.b64encode(buffered.getvalue()).decode()

{
  "field_definitions": {
    "mfe_price": "Maximum favorable price reached during the time the position was open",
    "mae_price": "Maximum adverse price reached during the time the position was open",
    "post_exit_excursion_price": "Most favorable price reached after the full position was closed"
  }
}


