def find_inefficient_trades(trades: list[dict], min_capture_pct: int = 70) -> dict:
    """
    Vind trades met lage MFE of PEEP capture percentage.

    Args:
        trades: List van trade dictionaries
        min_capture_pct: Minimale capture % threshold (default 70)

    Returns:
        Dictionary met inefficiënte trades gecategoriseerd
    """

    inefficient = {
        "low_mfe_capture": [],
        "low_peep_capture": [],
        "both_low": [],
        "summary": {}
    }

    for trade in trades:
        # Skip losses - die hebben 0% capture
        if trade["realized_r"] <= 0:
            continue

        low_mfe = trade["mfe_capture_pct"] < min_capture_pct
        low_peep = (trade["post_exit_excursion_price"] > 0 and
                    trade["post_exit_excursion_capture_pct"] < min_capture_pct)

        trade_info = {
            "trade_id": trade["trade_id"],
            "date": trade["date"],
            "instrument": trade["instrument"],
            "direction": trade["direction"],
            "realized_r": trade["realized_r"],
            "mfe_capture_pct": trade["mfe_capture_pct"],
            "peep_capture_pct": trade["post_exit_excursion_capture_pct"],
            "win_quality": trade["outcome"]["win_quality"],
            "missed_r": round(trade["max_possible_r"] - trade["realized_r"], 2)
        }

        if low_mfe and low_peep:
            inefficient["both_low"].append(trade_info)
        elif low_mfe:
            inefficient["low_mfe_capture"].append(trade_info)
        elif low_peep:
            inefficient["low_peep_capture"].append(trade_info)

    # Summary statistics
    total_inefficient = (len(inefficient["low_mfe_capture"]) +
                         len(inefficient["low_peep_capture"]) +
                         len(inefficient["both_low"]))

    total_wins = sum(1 for t in trades if t["realized_r"] > 0)

    inefficient["summary"] = {
        "total_inefficient": total_inefficient,
        "total_wins": total_wins,
        "inefficiency_rate": round((total_inefficient / total_wins * 100) if total_wins > 0 else 0, 1),
        "low_mfe_only": len(inefficient["low_mfe_capture"]),
        "low_peep_only": len(inefficient["low_peep_capture"]),
        "both_low": len(inefficient["both_low"]),
        "threshold_used": min_capture_pct
    }

    return inefficient


def print_inefficient_trades(inefficient: dict, show_details: bool = True):
    """Print inefficiënte trades in een leesbaar format"""

    print("\n" + "=" * 80)
    print("📊 INEFFICIËNTE TRADES RAPPORT")
    print("=" * 80)

    summary = inefficient["summary"]
    print(f"\n🎯 Threshold: {summary['threshold_used']}% capture")
    print(f"📈 Totaal winning trades: {summary['total_wins']}")
    print(f"⚠️  Inefficiënte trades: {summary['total_inefficient']} ({summary['inefficiency_rate']}%)")
    print(f"   - Alleen lage MFE: {summary['low_mfe_only']}")
    print(f"   - Alleen lage PEEP: {summary['low_peep_only']}")
    print(f"   - Beide laag: {summary['both_low']}")

    if show_details:
        # Beide laag - meest problematisch
        if inefficient["both_low"]:
            print("\n" + "=" * 80)
            print("🚨 TRADES MET BEIDE CAPTURE % LAAG (meest problematisch)")
            print("=" * 80)
            for t in sorted(inefficient["both_low"], key=lambda x: x["mfe_capture_pct"]):
                print(f"\n{t['trade_id']} - {t['date']}")
                print(
                    f"  {t['instrument']} {t['direction'].upper()} | Realized: {t['realized_r']}R | Missed: {t['missed_r']}R")
                print(f"  MFE capture: {t['mfe_capture_pct']}% | PEEP capture: {t['peep_capture_pct']}%")
                print(f"  Outcome: {t['win_quality']}")

        # Alleen lage MFE
        if inefficient["low_mfe_capture"]:
            print("\n" + "=" * 80)
            print("⚠️  TRADES MET LAGE MFE CAPTURE (te vroeg uitgestapt)")
            print("=" * 80)
            for t in sorted(inefficient["low_mfe_capture"], key=lambda x: x["mfe_capture_pct"]):
                print(f"\n{t['trade_id']} - {t['date']}")
                print(
                    f"  {t['instrument']} {t['direction'].upper()} | Realized: {t['realized_r']}R | Missed: {t['missed_r']}R")
                print(f"  MFE capture: {t['mfe_capture_pct']}% | PEEP capture: {t['peep_capture_pct']}%")
                print(f"  Outcome: {t['win_quality']}")

        # Alleen lage PEEP
        if inefficient["low_peep_capture"]:
            print("\n" + "=" * 80)
            print("💨 TRADES MET LAGE PEEP CAPTURE (gemiste follow-through)")
            print("=" * 80)
            for t in sorted(inefficient["low_peep_capture"], key=lambda x: x["peep_capture_pct"]):
                print(f"\n{t['trade_id']} - {t['date']}")
                print(
                    f"  {t['instrument']} {t['direction'].upper()} | Realized: {t['realized_r']}R | Missed: {t['missed_r']}R")
                print(f"  MFE capture: {t['mfe_capture_pct']}% | PEEP capture: {t['peep_capture_pct']}%")
                print(f"  Outcome: {t['win_quality']}")

    print("\n" + "=" * 80)


def analyze_exit_efficiency(trades: list[dict]):
    """Diepere analyse van exit efficiency patronen"""

    inefficient = find_inefficient_trades(trades, min_capture_pct=70)

    # Analyse per instrument
    print("\n📊 EXIT EFFICIENCY PER INSTRUMENT")
    print("=" * 60)

    for instrument in ["ES", "NQ"]:
        inst_trades = [t for t in trades if t["instrument"] == instrument and t["realized_r"] > 0]
        inst_inefficient = [t for t in inefficient["low_mfe_capture"] + inefficient["both_low"]
                            if t["instrument"] == instrument]

        if inst_trades:
            rate = len(inst_inefficient) / len(inst_trades) * 100
            avg_mfe = sum(t["mfe_capture_pct"] for t in inst_trades) / len(inst_trades)
            print(f"\n{instrument}:")
            print(f"  Inefficiency rate: {rate:.1f}%")
            print(f"  Avg MFE capture: {avg_mfe:.1f}%")

    # Analyse per win_quality
    print("\n📊 EXIT EFFICIENCY PER WIN QUALITY")
    print("=" * 60)

    for quality in ["full_target", "partial_target"]:
        quality_trades = [t for t in trades if t["outcome"]["win_quality"] == quality]
        quality_inefficient = [t for t in inefficient["low_mfe_capture"] + inefficient["both_low"]
                               if t["win_quality"] == quality]

        if quality_trades:
            rate = len(quality_inefficient) / len(quality_trades) * 100
            avg_mfe = sum(t["mfe_capture_pct"] for t in quality_trades) / len(quality_trades)
            print(f"\n{quality}:")
            print(f"  Inefficiency rate: {rate:.1f}%")
            print(f"  Avg MFE capture: {avg_mfe:.1f}%")

    # Top 5 meest inefficiënte trades
    print("\n🔥 TOP 5 MEEST INEFFICIËNTE TRADES")
    print("=" * 60)

    all_inefficient = (inefficient["low_mfe_capture"] +
                       inefficient["low_peep_capture"] +
                       inefficient["both_low"])

    top_5 = sorted(all_inefficient, key=lambda x: x["missed_r"], reverse=True)[:5]

    for i, t in enumerate(top_5, 1):
        print(f"\n{i}. {t['trade_id']} - {t['date']}")
        print(f"   {t['instrument']} {t['direction']} | Missed {t['missed_r']}R potential")
        print(f"   MFE: {t['mfe_capture_pct']}% | PEEP: {t['peep_capture_pct']}%")


# ===== USAGE =====

# Basic usage
inefficient = find_inefficient_trades(trades, min_capture_pct=70)
print_inefficient_trades(inefficient)

# Uitgebreide analyse
analyze_exit_efficiency(trades)

# Custom threshold
inefficient_50 = find_inefficient_trades(trades, min_capture_pct=50)
print_inefficient_trades(inefficient_50, show_details=False)

# Export naar CSV voor analyse in Excel
import csv


def export_inefficient_to_csv(inefficient: dict, filename: str):
    """Export inefficiënte trades naar CSV"""
    all_inefficient = (inefficient["low_mfe_capture"] +
                       inefficient["low_peep_capture"] +
                       inefficient["both_low"])

    with open(filename, 'w', newline='') as f:
        writer = csv.DictWriter(f, fieldnames=[
            'trade_id', 'date', 'instrument', 'direction',
            'realized_r', 'mfe_capture_pct', 'peep_capture_pct',
            'win_quality', 'missed_r'
        ])
        writer.writeheader()
        writer.writerows(all_inefficient)

    print(f"✅ Exported {len(all_inefficient)} inefficient trades to {filename}")


# Export
export_inefficient_to_csv(inefficient, "inefficient_trades.csv")
